<% console.log(dados); %>
<table>
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nome</th>
      <th scope="col">E-mail</th>
      <th scope="col">Celular</th>
    </tr>
  </thead>
  <tbody>
    <% let index=1; dados.forEach(user => { %>
    <tr <%- req.session.username==user.username ? `style="color: white; font-style: bold"` : `` %>>
      <td><%= user.id %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.celular %></td>
      <td><%= user.password %></td>
    </tr>
    <% index++; }); %>
  </tbody>
  <tfoot>
    <tr>
      <th colspan="5" style="font-size: 16px">
        <%= `UsuÃ¡rios registrados: ${index-1}` %>
      </th>
    </tr>
  </tfoot>
</table>